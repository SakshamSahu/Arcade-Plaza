[32m+[m[32mclass GamesListScreen extends StatefulWidget {[m
[32m+[m[32m  static const routeName = '/home-screen';[m
[32m+[m
[32m+[m[32m  @override[m
[32m+[m[32m  _GamesListScreenState createState() => _GamesListScreenState();[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mclass _GamesListScreenState extends State<GamesListScreen> {[m
[32m+[m[32m  Map _source = {ConnectivityResult.none: false};[m
[32m+[m[32m  MyConnectivity _connectivity = MyConnectivity.instance;[m
[32m+[m
[32m+[m[32m  @override[m
[32m+[m[32m  void initState() {[m
[32m+[m[32m    super.initState();[m
[32m+[m[32m    FacebookAudienceNetwork.init();[m
[32m+[m[32m    _loadInterstitialAd();[m
[32m+[m[32m    _connectivity.initialise();[m
[32m+[m[32m    _connectivity.myStream.listen((source) {[m
[32m+[m[32m      setState(() => _source = source);[m
[32m+[m[32m    });[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  bool _isInterstitialAdLoaded = false;[m
[32m+[m[32m  void _loadInterstitialAd() {[m
[32m+[m[32m    print("ad load is called");[m
[32m+[m[32m    FacebookInterstitialAd.loadInterstitialAd([m
[32m+[m[32m      placementId: AdHelper.interstitialAdId,[m
[32m+[m[32m      listener: (result, value) {[m
[32m+[m[32m        print(">> FAN > Interstitial Ad: $result --> $value");[m
[32m+[m[32m        if (result == InterstitialAdResult.LOADED) {[m
[32m+[m[32m          print("ad is loaded");[m
[32m+[m[32m          _isInterstitialAdLoaded = true;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        /// Once an Interstitial Ad has been dismissed and becomes invalidated,[m
[32m+[m[32m        /// load a fresh Ad by calling this function.[m
[32m+[m[32m        if (result == InterstitialAdResult.DISMISSED &&[m
[32m+[m[32m            value["invalidated"] == true) {[m
[32m+[m[32m          _isInterstitialAdLoaded = false;[m
[32m+[m[32m          _loadInterstitialAd();[m
[32m+[m[32m        }[m
[32m+[m[32m      },[m
[32m+[m[32m    );[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  @override[m
[32m+[m[32m  void dispose() {[m
[32m+[m[32m    super.dispose();[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  @override[m
[32m+[m[32m  Widget build(BuildContext context) {[m
[32m+[m[32m    String _conectivityStatus;[m
[32m+[m[32m    switch (_source.keys.toList()[0]) {[m
[32m+[m[32m      case ConnectivityResult.none:[m
[32m+[m[32m        _conectivityStatus = "Offline";[m
[32m+[m[32m        break;[m
[32m+[m[32m      case ConnectivityResult.mobile:[m
[32m+[m[32m        _conectivityStatus = "Online";[m
[32m+[m[32m        break;[m
[32m+[m[32m      case ConnectivityResult.wifi:[m
[32m+[m[32m        _conectivityStatus = "Online";[m
[32m+[m[32m    }[m
[32m+[m[32m    SystemChrome.setPreferredOrientations([DeviceOrientation.portraitUp]);[m
[32m+[m[32m    print(games.length);[m
[32m+[m[32m    return Scaffold([m
[32m+[m[32m        appBar: AppBar([m
[32m+[m[32m          title: const Text('Arcade Plaza'),[m
[32m+[m[32m          centerTitle: true,[m
[32m+[m[32m        ),[m
[32m+[m[32m        drawer: MainDrawer(),[m
[32m+[m[32m        body: DoubleBackToCloseApp([m
[32m+[m[32m          snackBar: SnackBar([m
[32m+[m[32m            content: Text('Tap back again to leave'),[m
[32m+[m[32m          ),[m
[32m+[m[32m          child: FutureBuilder([m
[32m+[m[32m            future: fetchFavorites(),[m
[32m+[m[32m            builder: (ctx, snapshot) =>[m
[32m+[m[32m                snapshot.connectionState == ConnectionState.done[m
[32m+[m[32m                    ? _conectivityStatus != 'Offline'[m
[32m+[m[32m                        ? Column([m
[32m+[m[32m                            children: [[m
[32m+[m[32m                              Expanded([m
[32m+[m[32m                                child: ListView.builder([m
[32m+[m[32m                                  padding: const EdgeInsets.all(5),[m
[32m+[m[32m                                  itemBuilder: (ctx, i) {[m
[32m+[m[32m                                    return InkWell([m
[32m+[m[32m                                      onTap: () {[m
[32m+[m[32m                                        if (_isInterstitialAdLoaded) {[m
[32m+[m[32m                                          FacebookInterstitialAd[m
[32m+[m[32m                                              .showInterstitialAd();[m
[32m+[m[32m                                        } else {[m
[32m+[m[32m                                          print("ad is not loaded");[m
[32m+[m[32m                                        }[m
[32m+[m[32m                                        Navigator.of(context).push([m
[32m+[m[32m                                            MaterialPageRoute(builder: (ctx) {[m
[32m+[m[32m                                          return GameScreen([m
[32m+[m[32m                                              games[i].title, games[i].gameUrl);[m
[32m+[m[32m                                        }));[m
[32m+[m[32m                                      },[m
[32m+[m[32m                                      child: ListItem([m
[32m+[m[32m                                        games[i].title,[m
[32m+[m[32m                                        games[i].imageUrl,[m
[32m+[m[32m                                      ),[m
[32m+[m[32m                                    );[m
[32m+[m[32m                                  },[m
[32m+[m[32m                                  itemCount: games.length,[m
[32m+[m[32m                                ),[m
[32m+[m[32m                              ),[m
[32m+[m[32m                              Container([m
[32m+[m[32m                                height: 50,[m
[32m+[m[32m                                child: FacebookBannerAd([m
[32m+[m[32m                                  bannerSize: BannerSize.STANDARD,[m
[32m+[m[32m                                  placementId: AdHelper.fbBannerAdId,[m
[32m+[m[32m                                ),[m
[32m+[m[32m                              )[m
[32m+[m[32m                            ],[m
[32m+[m[32m                          )[m
[32m+[m[32m                        : NoInternet()[m
[32m+[m[32m                    : Center([m
[32m+[m[32m                        child: CircularProgressIndicator.adaptive(),[m
[32m+[m[32m                      ),[m
[32m+[m[32m          ),[m
[32m+[m[32m        ),[m
[32m+[m[32m        backgroundColor: Colors.amber);[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
