[1m--- /dev/null[m
[1m+++ b/lib/models/games.dart[m
[36m@@ -0,0 +1,449 @@[m
[32m+[m[32mimport 'package:shared_preferences/shared_preferences.dart';[m
[32m+[m
[32m+[m[32mclass Game {[m
[32m+[m[32m  final String imageUrl;[m
[32m+[m[32m  final String title;[m
[32m+[m[32m  final String gameUrl;[m
[32m+[m[32m  Game([m
[32m+[m[32m    this.title,[m
[32m+[m[32m    this.imageUrl,[m
[32m+[m[32m    this.gameUrl,[m
[32m+[m[32m  );[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mList<Game> favoriteGames = [];[m
[32m+[m
[32m+[m[32mList<String> get favoriteGamesTitle {[m
[32m+[m[32m  return favoriteGames.map((e) => e.title).toList();[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mFuture<void> saveFavorites() async {[m
[32m+[m[32m  // print('save favorites is called');[m
[32m+[m[32m  final prefs = await SharedPreferences.getInstance();[m
[32m+[m[32m  // print(favoriteGamesTitle);[m
[32m+[m[32m  prefs.setStringList('favoriteTitles', favoriteGamesTitle);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mFuture<void> fetchFavorites() async {[m
[32m+[m[32m  // print('fetch favorites is called');[m
[32m+[m[32m  final prefs = await SharedPreferences.getInstance();[m
[32m+[m[32m  final fetchedFavTitles = prefs.getStringList('favoriteTitles');[m
[32m+[m[32m  favoriteGames = games[m
[32m+[m[32m      .where((element) => fetchedFavTitles.contains(element.title))[m
[32m+[m[32m      .toList();[m
[32m+[m[32m  // print(fetchedFavTitles);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mbool isGameFavorite(String title) {[m
[32m+[m[32m  return favoriteGames.any((element) => element.title == title);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mvoid toggleFavorite(String title) {[m
[32m+[m[32m  int index = favoriteGames.indexWhere((element) => element.title == title);[m
[32m+[m[32m  if (index != -1) {[m
[32m+[m[32m    favoriteGames.removeAt(index);[m
[32m+[m[32m  } else {[m
[32m+[m[32m    favoriteGames.add(games.firstWhere((element) => element.title == title));[m
[32m+[m[32m  }[m
[32m+[m[32m  saveFavorites();[m
[32m+[m[32m}[m
