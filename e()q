[32m+[m[32mimport 'package:flutter/material.dart';[m
[32m+[m[32mimport 'package:double_back_to_close_app/double_back_to_close_app.dart';[m
[32m+[m[32mimport 'package:connectivity/connectivity.dart';[m
[32m+[m[32mimport '../widgets/fav_item.dart';[m
[32m+[m[32mimport '../widgets/no_internet.dart';[m
[32m+[m[32mimport '../models/connectivity.dart';[m
[32m+[m[32mimport 'package:google_mobile_ads/google_mobile_ads.dart';[m
[32m+[m[32mimport 'package:facebook_audience_network/facebook_audience_network.dart';[m
[32m+[m[32mimport 'package:web_display/models/ad_helper.dart';[m
[32m+[m[32mimport './main_drawer.dart';[m
[32m+[m[32mimport '../models/games.dart';[m
[32m+[m[32mimport '../screens/game_screen.dart';[m
[32m+[m
[32m+[m[32mclass FavoriteGamesScreen extends StatefulWidget {[m
[32m+[m[32m  static const routeName = '/fav-screen';[m
[32m+[m
[32m+[m[32m  @override[m
[32m+[m[32m  _FavoriteGamesScreenState createState() => _FavoriteGamesScreenState();[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mclass _FavoriteGamesScreenState extends State<FavoriteGamesScreen> {[m
[32m+[m[32m  Map _source = {ConnectivityResult.none: false};[m
[32m+[m[32m  MyConnectivity _connectivity = MyConnectivity.instance;[m
[32m+[m[32m  BannerAd _bannerAd;[m
[32m+[m[32m  @override[m
[32m+[m[32m  void initState() {[m
[32m+[m[32m    super.initState();[m
[32m+[m[32m    _connectivity.initialise();[m
[32m+[m[32m    _connectivity.myStream.listen((source) {[m
[32m+[m[32m      setState(() => _source = source);[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    FacebookAudienceNetwork.init();[m
[32m+[m[32m    _loadInterstitialAd();[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  @override[m
[32m+[m[32m  void didChangeDependencies() {[m
[32m+[m[32m    super.didChangeDependencies();[m
[32m+[m[32m    _bannerAd = BannerAd([m
[32m+[m[32m      size: AdSize.banner,[m
[32m+[m[32m      adUnitId: AdHelper.bannerAdUnitId,[m
[32m+[m[32m      listener: BannerAdListener(),[m
[32m+[m[32m      request: AdRequest(),[m
[32m+[m[32m    )..load();[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  bool _isInterstitialAdLoaded = false;[m
[32m+[m[32m  void _loadInterstitialAd() {[m
[32m+[m[32m    FacebookInterstitialAd.loadInterstitialAd([m
[32m+[m[32m      placementId: AdHelper[m
[32m+[m[32m          .interstitialAdId, //"IMG_16_9_APP_INSTALL#2312433698835503_2650502525028617" YOUR_PLACEMENT_ID[m
[32m+[m[32m      listener: (result, value) {[m
[32m+[m[32m        print(">> FAN > Interstitial Ad: $result --> $value");[m
[32m+[m[32m        if (result == InterstitialAdResult.LOADED) {[m
[32m+[m[32m          print("ad is loaded");[m
[32m+[m[32m          _isInterstitialAdLoaded = true;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        /// Once an Interstitial Ad has been dismissed and becomes invalidated,[m
[32m+[m[32m        /// load a fresh Ad by calling this function.[m
[32m+[m[32m        if (result == InterstitialAdResult.DISMISSED &&[m
[32m+[m[32m            value["invalidated"] == true) {[m
[32m+[m[32m          _isInterstitialAdLoaded = false;[m
[32m+[m[32m          _loadInterstitialAd();[m
[32m+[m[32m        }[m
[32m+[m[32m      },[m
[32m+[m[32m    );[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  @override[m
[32m+[m[32m  Widget build(BuildContext context) {[m
[32m+[m[32m    String _conectivityStatus;[m
[32m+[m[32m    switch (_source.keys.toList()[0]) {[m
[32m+[m[32m      case ConnectivityResult.none:[m
[32m+[m[32m        _conectivityStatus = "Offline";[m
[32m+[m[32m        break;[m
[32m+[m[32m      case ConnectivityResult.mobile:[m
[32m+[m[32m        _conectivityStatus = "Online";[m
[32m+[m[32m        break;[m
[32m+[m[32m      case ConnectivityResult.wifi:[m
[32m+[m[32m        _conectivityStatus = "Online";[m
[32m+[m[32m    }[m
[32m+[m[32m    return Scaffold([m
[32m+[m[32m        appBar: AppBar([m
[32m+[m[32m          title: const Text('My Favorites'),[m
[32m+[m[32m          centerTitle: true,[m
[32m+[m[32m        ),[m
[32m+[m[32m        backgroundColor: Colors.amber,[m
[32m+[m[32m        drawer: MainDrawer(),[m
[32m+[m[32m        body: DoubleBackToCloseApp([m
[32m+[m[32m          snackBar: SnackBar(content: Text('Tap back again to leave')),[m
[32m+[m[32m          child: favoriteGames.isEmpty[m
[32m+[m[32m              ? Center([m
[32m+[m[32m                  child: const Text([m
[32m+[m[32m                      'You dont have any favorite games, try adding some'),[m
[32m+[m[32m                )[m
[32m+[m[32m              : _conectivityStatus == 'Online'[m
[32m+[m[32m                  ? Column([m
[32m+[m[32m                      children: [[m
[32m+[m[32m                        Expanded([m
[32m+[m[32m                          child: ListView.builder([m
[32m+[m[32m                            itemBuilder: (ctx, i) {[m
[32m+[m[32m                              return InkWell([m
[32m+[m[32m                                onTap: () {[m
[32m+[m[32m                                  if (_isInterstitialAdLoaded) {[m
[32m+[m[32m                                    FacebookInterstitialAd.showInterstitialAd();[m
[32m+[m[32m                                  } else {[m
[32m+[m[32m                                    print("ad is not loaded");[m
[32m+[m[32m                                  }[m
[32m+[m[32m                                  Navigator.of(context)[m
[32m+[m[32m                                      .push(MaterialPageRoute(builder: (ctx) {[m
[32m+[m[32m                                    return GameScreen(favoriteGames[i].title,[m
[32m+[m[32m                                        favoriteGames[i].gameUrl);[m
[32m+[m[32m                                  }));[m
[32m+[m[32m                                },[m
[32m+[m[32m                                child: FavItem([m
[32m+[m[32m                                  favoriteGames[i].title,[m
[32m+[m[32m                                  favoriteGames[i].imageUrl,[m
[32m+[m[32m                                ),[m
[32m+[m[32m                              );[m
[32m+[m[32m                            },[m
[32m+[m[32m                            itemCount: favoriteGames.length,[m
[32m+[m[32m                          ),[m
[32m+[m[32m                        ),[m
[32m+[m[32m                        if (_bannerAd != null)[m
[32m+[m[32m                          Container([m
[32m+[m[32m                            height: 50,[m
[32m+[m[32m                            child: AdWidget(ad: _bannerAd),[m
[32m+[m[32m                          )[m
[32m+[m[32m                      ],[m
[32m+[m[32m                    )[m
[32m+[m[32m                  : NoInternet(),[m
[32m+[m[32m        ));[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
