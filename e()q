[1mnew file mode 100644[m
[1mindex 0000000..47658d6[m
[1m--- /dev/null[m
[1m+++ b/lib/models/ad_helper.dart[m
[36m@@ -0,0 +1,30 @@[m
[32m+[m[32mimport 'dart:io';[m
[32m+[m[32m// import 'package:facebook_audience_network/facebook_audience_network.dart';[m
[32m+[m
[32m+[m[32mclass AdHelper {[m
[32m+[m[32m//facebook ads[m
[32m+[m[32m  static String get interstitialAdId {[m
[32m+[m[32m    if (Platform.isAndroid) {[m
[32m+[m[32m      return "296800735446599_296803568779649";[m
[32m+[m[32m    }[m
[32m+[m[32m    return ""; //ios id must be here[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m//google ads[m
[32m+[m[32m  static String get bannerAdUnitId {[m
[32m+[m[32m    if (Platform.isAndroid) {[m
[32m+[m[32m      return 'ca-app-pub-3140938214583211/5818873432';[m
[32m+[m[32m    } else if (Platform.isIOS) {[m
[32m+[m[32m      return '';[m
[32m+[m[32m    } else {[m
[32m+[m[32m      throw UnsupportedError('Unsupported platform');[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  static String get fbBannerAdId {[m
[32m+[m[32m    if (Platform.isAndroid) {[m
[32m+[m[32m      return "296800735446599_301410258318980";[m
[32m+[m[32m    }[m
[32m+[m[32m    return "";[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
