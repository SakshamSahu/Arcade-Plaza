[32m+[m[32mimport 'package:flutter/material.dart';[m
[32m+[m[32mimport 'package:google_mobile_ads/google_mobile_ads.dart';[m
[32m+[m[32mimport './screens/favorite_games_screen.dart';[m
[32m+[m[32mimport './screens/games_list_screen.dart';[m
[32m+[m
[32m+[m[32mvoid main() {[m
[32m+[m[32m  WidgetsFlutterBinding.ensureInitialized();[m
[32m+[m[32m  MobileAds.instance.initialize();[m
[32m+[m[32m  runApp(MyApp());[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mclass MyApp extends StatelessWidget {[m
[32m+[m[32m  @override[m
[32m+[m[32m  Widget build(BuildContext context) {[m
[32m+[m[32m    return MaterialApp([m
[32m+[m[32m      title: 'Arcade Plaza',[m
[32m+[m[32m      theme: ThemeData([m
[32m+[m[32m        fontFamily: 'Cairo',[m
[32m+[m[32m        primarySwatch: Colors.pink,[m
[32m+[m[32m        accentColor: Colors.amber,[m
[32m+[m[32m      ),[m
[32m+[m[32m      home: GamesListScreen(),[m
[32m+[m[32m      debugShowCheckedModeBanner: false,[m
[32m+[m[32m      routes: {[m
[32m+[m[32m        FavoriteGamesScreen.routeName: (ctx) => FavoriteGamesScreen(),[m
[32m+[m[32m        GamesListScreen.routeName: (ctx) => GamesListScreen(),[m
[32m+[m[32m      },[m
[32m+[m[32m    );[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
