[32m+[m[32mimport 'dart:io';[m
[32m+[m[32mimport 'dart:async';[m
[32m+[m[32mimport 'package:connectivity/connectivity.dart';[m
[32m+[m
[32m+[m[32mclass MyConnectivity {[m
[32m+[m[32m  MyConnectivity._internal();[m
[32m+[m
[32m+[m[32m  static final MyConnectivity _instance = MyConnectivity._internal();[m
[32m+[m
[32m+[m[32m  static MyConnectivity get instance => _instance;[m
[32m+[m
[32m+[m[32m  Connectivity connectivity = Connectivity();[m
[32m+[m
[32m+[m[32m  StreamController controller = StreamController.broadcast();[m
[32m+[m
[32m+[m[32m  Stream get myStream => controller.stream;[m
[32m+[m
[32m+[m[32m  void initialise() async {[m
[32m+[m[32m    ConnectivityResult result = await connectivity.checkConnectivity();[m
[32m+[m[32m    _checkStatus(result);[m
[32m+[m[32m    connectivity.onConnectivityChanged.listen((result) {[m
[32m+[m[32m      _checkStatus(result);[m
[32m+[m[32m    });[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  void _checkStatus(ConnectivityResult result) async {[m
[32m+[m[32m    bool isOnline = false;[m
[32m+[m[32m    try {[m
[32m+[m[32m      final result = await InternetAddress.lookup('www.google.com/');[m
[32m+[m[32m      if (result.isNotEmpty && result[0].rawAddress.isNotEmpty) {[m
[32m+[m[32m        isOnline = true;[m
[32m+[m[32m      } else[m
[32m+[m[32m        isOnline = false;[m
[32m+[m[32m    } on SocketException catch (_) {[m
[32m+[m[32m      isOnline = false;[m
[32m+[m[32m    }[m
[32m+[m[32m    controller.sink.add({result: isOnline});[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  static void getConnectedStatus(Map _source, String _conectivityStatus) {}[m
[32m+[m
[32m+[m[32m  void disposeStream() => controller.close();[m
[32m+[m[32m}[m
